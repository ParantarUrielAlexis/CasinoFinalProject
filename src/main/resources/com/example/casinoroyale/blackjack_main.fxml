<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.casinoroyale.BlackJackMain">
   <children>
      <ImageView fx:id="ivBackground" fitHeight="1080.0" fitWidth="1920.0" pickOnBounds="true">
         <image>
            <Image url="@../../../BlackJack/BACK.jpg" />
         </image>
      </ImageView>
      <VBox layoutX="884.0" layoutY="722.0" prefHeight="126.0" prefWidth="152.0">
         <children>
            <Button fx:id="btnStart" mnemonicParsing="false" onAction="#btnStartOnAction" prefHeight="65.0" prefWidth="150.0" text="Start">
               <font>
                  <Font name="Times New Roman" size="35.0" />
               </font>
            </Button>
            <Button fx:id="btnExit" mnemonicParsing="false" onAction="#btnExitOnAction" prefHeight="50.0" prefWidth="150.0" text="Exit">
               <font>
                  <Font name="Times New Roman" size="35.0" />
               </font>
            </Button>
         </children>
      </VBox>
      <ImageView fitHeight="402.0" fitWidth="684.0" layoutX="660.0" layoutY="32.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../BlackJack/titleimage.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
